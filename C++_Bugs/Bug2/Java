import java.util.*;

public class Main {
    public static void main(String[] args) {
        int a = 0; // Initialize a
        int b;
        int c = 0;
        if (a + 2 != 0)
            c = 1;
        int d = (1 + c - 2 + (c == 1 ? 1 : 0)) - 1;
        b = ((d + 1) << d) + d;
        System.out.printf("%X%n", b);
    }
}
