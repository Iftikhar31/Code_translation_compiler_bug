using System;

namespace CodeTranslation
{
    public class Program
    {
        // Function equivalent to the Python function foo(n)
        public static void Foo(int n)
        {
            while (n < 3)
            {
                Console.WriteLine(n);
                n += 1;
            }
            return;
        }

        public static void Main(string[] args)
        {
            Foo(0);
        }
    }
}
